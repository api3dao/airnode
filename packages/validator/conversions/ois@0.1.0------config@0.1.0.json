{
  "__template": "../templates/0.1.0/ois.json",
  "__actions": [
    {
      "__insert": {
        "__target": "[]",
        "__value": [
          {
            "id": "{FILL}",
            "nodeSettings": {
              "nodeVersion": "{FILL}",
              "cloudProvider": "aws",
              "region": "{FILL}",
              "stage": "{FILL}",
              "logFormat": "{FILL}",
              "logLevel": "{FILL}"
            },
            "environment": {
              "securitySchemes": [],
              "chainProviders": []
            },
            "chains": [],
            "triggers": {
              "request": []
            },
            "ois": []
          }
        ]
      }
    },
    {
      "__copy": {
        "__target": "[ '[_]', 'ois[]' ]"
      }
    }
  ],
  "apiSpecifications": {
    "security": {
      "__conditions": [
        {
          "__if": {
            "__this_name": ".*"
          },
          "__then": {
            "__actions": [
              {
                "__insert": {
                  "__target": "[ '[_]', 'environment', 'securitySchemes[]', 'name' ]",
                  "__value": "__match"
                }
              },
              {
                "__insert": {
                  "__target": "[ '[_]', 'environment', 'securitySchemes[_]', 'oisTitle' ]",
                  "__value": "[[ '/', 'title' ]]"
                }
              },
              {
                "__insert": {
                  "__target": "[ '[_]', 'environment', 'securitySchemes[_]', 'envName' ]",
                  "__value": "ss_[[ '/', 'title' ]]___match"
                }
              }
            ]
          }
        }
      ]
    }
  },
  "endpoints": {
    "__arrayItem": {
      "name": {
        "__actions": [
          {
            "__insert": {
              "__target": "[ '[_]', 'triggers', 'request[]', 'endpointId' ]",
              "__value": "{FILL}"
            }
          },
          {
            "__copy": {
              "__target": "[ '[_]', 'triggers', 'request[_]', 'endpointName' ]"
            }
          },
          {
            "__insert": {
              "__target": "[ '[_]', 'triggers', 'request[_]', 'oisTitle' ]",
              "__value": "[[ '/', 'title' ]]"
            }
          }
        ]
      }
    }
  }
}
