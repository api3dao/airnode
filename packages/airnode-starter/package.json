{
  "name": "@api3/airnode-starter",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A starter project for deploying an Airnode and making requests to it",
  "scripts": {
    "compile": "hardhat compile",
    "eth-node": "hardhat node --no-deploy",
    "deploy:localhost": "hardhat deploy --network localhost",
    "deploy:ropsten": "hardhat deploy --network ropsten",
    "deploy:rinkeby": "hardhat deploy --network rinkeby",
    "deploy:goerli": "hardhat deploy --network goerli",
    "deploy:xdai": "hardhat deploy --network xdai",
    "deploy:fantom": "hardhat deploy --network fantom",
    "create-airnode-secrets": "hardhat run scripts/create-airnode-secrets.ts",
    "deploy-requester": "hardhat run scripts/deploy-requester.ts",
    "mak-request": "hardhat run scripts/mak-request.ts",
    "deployer": "deployer",
    "admin": "admin"
  },
  "dependencies": {
    "@api3/admin": "^0.1.0",
    "@api3/airnode-abi": "^0.1.0",
    "@api3/deployer": "^0.1.0",
    "@api3/protocol": "^0.1.0",
    "ethers": "^5.4.4"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@types/node": "^16.9.2",
    "@types/prompts": "^2.0.14",
    "ethereum-waffle": "^3.4.0",
    "hardhat": "^2.6.0",
    "hardhat-deploy": "^0.9.0",
    "prompts": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
