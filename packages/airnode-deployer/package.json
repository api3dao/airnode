{
  "name": "@api3/airnode-deployer",
  "license": "MIT",
  "version": "0.10.0",
  "private": false,
  "bin": {
    "airnode-deployer": "./dist/bin/deployer.js"
  },
  "main": "./dist/bin/deployer.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile && yarn webpack && yarn copy:webpack && yarn copy:terraform",
    "clean": "rimraf -g *.tsbuildinfo ./dist ./.webpack ./build *.tgz",
    "cli": "ts-node bin/deployer.ts",
    "compile": "tsc --build tsconfig.json",
    "copy:terraform": "copyfiles terraform/**/**/*.tf terraform/**/**/*.tpl  dist/",
    "copy:webpack": "copyfiles .webpack/**/*.js .webpack/templates/**/*.json .webpack/conversions/**/*.json dist/",
    "pack": "yarn pack",
    "test": "jest --coverage",
    "webpack": "webpack",
    "webpack:dev": "webpack --mode development"
  },
  "dependencies": {
    "@api3/airnode-abi": "^0.10.0",
    "@api3/airnode-node": "^0.10.0",
    "@api3/airnode-protocol": "^0.10.0",
    "@api3/airnode-utilities": "^0.10.0",
    "@api3/airnode-validator": "^0.10.0",
    "@api3/promise-utils": "^0.3.0",
    "@google-cloud/storage": "^6.9.4",
    "adm-zip": "^0.5.10",
    "aws-sdk": "^2.1333.0",
    "chalk": "^4.1.2",
    "cli-table3": "^0.6.3",
    "compare-versions": "^5.0.3",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "lodash": "^4.17.21",
    "ora": "^5.4.1",
    "yargs": "^17.7.1",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.3",
    "@types/adm-zip": "^0.5.0",
    "@types/aws-lambda": "^8.10.111",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.0",
    "@types/yargs": "^17.0.22",
    "copyfiles": "^2.4.1",
    "esbuild-loader": "^3.0.1",
    "jest": "^29.5.0",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-ignore-dynamic-require": "^1.0.0"
  }
}
